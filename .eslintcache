get.checked\n    }\n    dispatch({ type: UPDATE_FILTERS, value: { name, value } })\n  }\n  const clearFilters = () => {\n    dispatch({ type: CLEAR_FILTERS })\n    console.log('clearFilters')\n  }\n\n  const productsView = (value) => {\n    dispatch({ type: SET_GRIDVIEW, value })\n  }\n\n  return (\n    <FilterContext.Provider value={{ ...state, productsView, updateSort, updateFilters, clearFilters }}>\n      {children}\n    </FilterContext.Provider>\n  )\n}\n// make sure use\nexport const useFilterContext = () => {\n  return useContext(FilterContext)\n}\n","C:\\Users\\hussa\\OneDrive\\Työpöytä\\store\\src\\App.js",[],"C:\\Users\\hussa\\OneDrive\\Työpöytä\\store\\src\\context\\cart_context.js",[],"C:\\Users\\hussa\\OneDrive\\Työpöytä\\store\\src\\context\\products_context.js",["214"],"import axios from 'axios'\nimport React, { useContext, useEffect, useReducer } from 'react'\nimport reducer from '../reducers/products_reducer'\nimport { products_url as url } from '../utils/constants'\nimport {\n  SIDEBAR_OPEN,\n  SIDEBAR_CLOSE,\n  GET_PRODUCTS_BEGIN,\n  GET_PRODUCTS_SUCCESS,\n  GET_PRODUCTS_ERROR,\n  GET_SINGLE_PRODUCT_BEGIN,\n  GET_SINGLE_PRODUCT_SUCCESS,\n  GET_SINGLE_PRODUCT_ERROR,\n} from '../actions'\nimport { FaSmile } from 'react-icons/fa'\n\nconst ProductsContext = React.createContext()\n\n\nexport const ProductsProvider = ({ children }) => {\n\n  const initialState = {\n    isSidebarOpen: false,\n    products_loading: false,\n    products_error: false,\n    products: [],\n    featured_products: [],\n    single_product_loading: false,\n    single_product_error: false,\n    single_product: {}\n  }\n  const [state, dispatch] = useReducer(reducer, initialState)\n\n  function closeSidebar() {\n    dispatch({ type: SIDEBAR_CLOSE })\n  }\n\n  function openSidebar() {\n    dispatch({ type: SIDEBAR_OPEN })\n  }\n  const fetchProducts = async (url) => {\n    dispatch({ type: GET_PRODUCTS_BEGIN })\n    try {\n      const response = await axios.get(url)\n      const pro